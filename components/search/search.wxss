/* components/search/search.wxss */
.search{
    width: 100%;
    height: 54px;
    padding: 10px 15px;
    box-sizing: border-box;
}
.search-input{
    background-color: #f2f2f2;
    border-radius: 40rpx;
    height: 34px;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
}
.icon-search{
    width: 14px;
    height: 14px;
}
.search-text{
    font-size: 14px;
    margin-left: 10rpx;
    color: #999;
}

