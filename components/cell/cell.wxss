/* components/cell/cell.wxss */
.cell{
    background: #fff;
    border-radius: 5px;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
    padding: 10px 16px 10px 10px;
    box-sizing: border-box;
    margin:0 0 10px 0;
}
.cell-book{
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
}
.cell-img-container{
    width: 40px;
    height: 50px;
    margin-right: 10px;
}
.cell-img-container .cell-img_01{
    width: 100%;
    height: 100%;
}
.cell-content{
    display: flex;
    flex-direction: column;
}
.cell-title{
    text-align: left;
    font-size: 14px;
}
.cell-desc{
    font-size: 10px;
    color: #999;
}
.cell-img_02{
    width: 16px;
    height: 14px;
}